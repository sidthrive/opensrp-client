<form autocomplete="off" novalidate="novalidate" class="or clearfix" dir="ltr" id="confirm_form">\n
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSL stylesheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 dir="auto" id="form-title">Confirm</h3>
    <select id="form-languages" data-default-lang="id">
        <option value="id" data-dir="ltr">Indonesia</option>
        <option value="en" data-dir="ltr">English</option>
    </select>
    <fieldset class="question simple-select ">
        <fieldset>
            <legend>
                <span lang="id" class="question-label active" data-itext-id="/confirm_forms/is_confirm:label">Svp Confirm</span>
                <span lang="en" class="question-label " data-itext-id="/confirm_forms/is_confirm:label">Svp Confirm</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input type="radio" name="/confirm_forms/is_confirm" data-name="/confirm_forms/is_confirm" value="1" data-type-xml="select1"/>
                    <span lang="id" class="option-label active" data-itext-id="/confirm_forms/is_confirm/1:label">Ya</span>
                    <span lang="en" class="option-label " data-itext-id="/confirm_forms/is_confirm/1:label">Yes</span>
                </label>
                <label class="">
                    <input type="radio" name="/confirm_forms/is_confirm" data-name="/confirm_forms/is_confirm" value="0" data-type-xml="select1"/>
                    <span lang="id" class="option-label active" data-itext-id="/confirm_forms/is_confirm/0:label">Tidak</span>
                    <span lang="en" class="option-label " data-itext-id="/confirm_forms/is_confirm/0:label">No</span>
                </label>
            </div>
        </fieldset>
    </fieldset> \n
    <fieldset id="or-calculated-items" style="display:none;">
        <label class="calculation non-select ">
            <input type="hidden" name="/confirm_forms/meta/instanceID" data-calculate="concat('uuid:', uuid())" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input type="hidden" name="/confirm_forms/formhub/uuid" data-calculate="'8654daf887e648d49a7f891e75c28b6b'" data-type-xml="string"/>
        </label>
    </fieldset>
</form>